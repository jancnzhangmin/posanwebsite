
    <div class="ibox-title">
      <h3>新闻</h3>
    </div>
    <div class="ibox-content">
      <table class="table table-hover table-bordered text-center">
        <thead>
        <tr>
          <th class="text-center">新闻图片</th>
          <th class="text-center">标题</th>
          <th class="text-center">创建时间</th>
          <th class="text-center">操作</th>
        </tr>
        </thead>
        <tbody>
        <% @newspapers.each do |newspaper| %>
            <tr>
              <td>

              <img width="100px" src=<%= newspaper.newspaperimage.url %> >
              </td>
              <td><%= newspaper.title %></td>
              <td><%= newspaper.created_at.strftime('%Y-%m-%d %H:%M:%S') %></td>

              <td><%= link_to '编辑',edit_newspaper_path(newspaper) %>



                <%= link_to '删除', newspaper, method: :delete, data: { confirm: '确认删除?' } %>

                    </td>
            </tr>
        <% end %>
        </tbody>
      </table>
      <%= link_to "新增",new_newspaper_path,class: "btn btn-w-m btn-primary pull-right" %>
      <br>
    </div>
