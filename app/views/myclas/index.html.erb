
    <div class="ibox-title">
      <h3>商品类</h3>
    </div>
    <div class="ibox-content">
      <table class="table table-hover table-bordered text-center">
        <thead>
        <tr>
          <th class="text-center">商品类型</th>


          <th class="text-center">操作</th>
        </tr>
        </thead>
        <tbody>
        <% @myclas.each do |mycla| %>
            <tr>

              <td><%= mycla.cla %></td>


              <td><%= link_to '编辑',edit_mycla_path(mycla) %>



                <%= link_to '删除', mycla, method: :delete, data: { confirm: '确认删除?' } %>

                    </td>
            </tr>
        <% end %>
        </tbody>
      </table>
      <%= link_to "新增",new_mycla_path,class: "btn btn-w-m btn-primary pull-right" %>
      <br>
    </div>
